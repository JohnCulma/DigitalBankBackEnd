          "schemaName": "dbo",
          "tableName": "DigitalBankLogs",
          "autoCreateSqlTable": true,
          "columnOptionsSection": {
            "removeStandardColumns": [ "Properties" ],
            "customColumns": [
              {
                "ColumnName": "stack trace",
                "DataType": "varchar",
                "DataLength": "max"
              },
              {
                "ColumnName": "Release",
                "DataType": "varchar",
                "DataLength": 32
              }
            ]
          }
        },
        "restrictedToMinimumLevel": "Warning"
      }
    ]
  }
